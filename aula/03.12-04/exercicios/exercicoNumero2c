/* ========================================================================= */
/* --- Bibliotecas --- */
#include <stdio.h>
#include <stdlib.h>

/* ========================================================================= */
/* --- Função Principal --- */
main()
{
    int alunos,
        alunas;

    printf("Digite o valor de alunos:");
    scanf("%d", &alunos);
    printf("Digite o valor de alunas ");
    scanf("%d", &alunas);
    printf("O numero de alunos e %d, e o numero de alunas e %d", alunas, alunos);

} /* end main */

/* ============================================================================ */
/* --- Final do Programa --- */